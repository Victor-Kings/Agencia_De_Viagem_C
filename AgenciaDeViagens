         {

             if(x.custo<custo_min)
             {
                 m=0;
                 custo_min=x.custo;
                 while(x.caminho[m]!=0)
                 {
                     caminho_min[m]=x.caminho[m];
                     m++;
                 }
             }
         }
         else
         {
             j=1;
             while(x.caminho[i-1]!=grafo[j][0])
             {
                 j++;
             }
             for(k=1;k<43;k++)
             {


                 if(grafo[j][k]!=0)
                 {
                     custo_res=x.custo;
                     m=0;
                  while(x.caminho[m]!=0)
                 {
                     caminho_res[m]=x.caminho[m];
                     m++;
                 }
                     g=0;
                    while(x.caminho[g]!=0)
                    {
                        if(x.caminho[g]==grafo[0][k])
                        {
                             flag=1;
                       }
                         g++;
                   }
                    if(flag==0)
                     {

                     x.custo+=grafo[j][k];
                     x.caminho[i]=grafo[0][k];
                     if(custo_min>x.custo)
                     Inserir_final(&p,x);
                     m=0;

                     x.custo=custo_res;
                     m=0;
                 while(caminho_res[m]!=0)
                 {
                     x.caminho[m]=caminho_res[m];
                     m++;
                 }
                    }
                     flag=0;
                 }
             }
         }

     }
     i=0;
    flag=0;
    while(caminho_min[i]!=0){
        for(j=0;flag=0||j<20;j++){
            if(caminho_min[i]==j){
                printf("%s - ",cidades[j]);
                flag=1;
            }
        }
        flag=0;

        i++;
    }

    printf("\n custo: %d",custo_min);
